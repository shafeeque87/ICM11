<?xml version="1.0" encoding="UTF-8"?>
<enfinity xmlns:dt="http://www.intershop.com/xml/ns/enfinity/6.5/core/impex-dt">
<pagelet-component id="cmp_categoryEndcaps_topRated">
<definition-name>app_sf_responsive_cm:component.common.productListFilter.pagelet2-Component</definition-name>
<display-name xml:lang="de-DE">Kategorie Endcaps Am besten bewertet</display-name>
<display-name xml:lang="en-US">Category Endcaps Top Rated</display-name>
<display-name xml:lang="fr-FR">Têtes de gondole de catégorie les mieux classés</display-name>
<online>true</online>
<author business-partner-no="admin" domain="PlanetOfPlants"/>
<configuration-parameters>
<configuration-parameter name="Filter">
<definition-name>app_sf_responsive_cm:component.common.productListFilter.pagelet2-Component-Filter</definition-name>
<value name="Filter">top_rated_products</value>
</configuration-parameter>
<configuration-parameter name="ListStyle">
<definition-name>app_sf_responsive_cm:component.common.productListFilter.pagelet2-Component-ListStyle</definition-name>
<value name="ListStyle">carousel</value>
</configuration-parameter>
<configuration-parameter name="MaxNumberOfProducts">
<definition-name>app_sf_responsive_cm:component.common.productListFilter.pagelet2-Component-MaxNumberOfProducts</definition-name>
<value name="MaxNumberOfProducts">9</value>
</configuration-parameter>
<configuration-parameter name="ProductView">
<definition-name>app_sf_responsive_cm:component.common.productListFilter.pagelet2-Component-ProductView</definition-name>
<value name="ProductView">app_sf_responsive:vc_productTile@PlanetOfPlants-MasterRepository</value>
</configuration-parameter>
<configuration-parameter name="Scope">
<definition-name>app_sf_responsive_cm:component.common.productListFilter.pagelet2-Component-Scope</definition-name>
<value name="Scope">CurrentCategoryScope</value>
</configuration-parameter>
<configuration-parameter name="ShowViewAll">
<definition-name>app_sf_responsive_cm:component.common.productListFilter.pagelet2-Component-ShowViewAll</definition-name>
<value name="ShowViewAll">true</value>
</configuration-parameter>
<configuration-parameter name="SlideItems">
<definition-name>app_sf_responsive_cm:component.common.productListFilter.pagelet2-Component-SlideItems</definition-name>
<value name="SlideItems">3</value>
</configuration-parameter>
<configuration-parameter name="Title">
<definition-name>app_sf_responsive_cm:component.common.productListFilter.pagelet2-Component-Title</definition-name>
<value name="Title" xml:lang="de-DE">Beste Bewertung</value>
<value name="Title" xml:lang="en-US">Top Rated</value>
<value name="Title" xml:lang="fr-FR">le mieux classé</value>
</configuration-parameter>
</configuration-parameters>
</pagelet-component>
</enfinity>
