<?xml version="1.0" encoding="UTF-8"?>
<enfinity xmlns:dt="http://www.intershop.com/xml/ns/enfinity/6.5/core/impex-dt">
<component-template id="cmp_tmp_headerCheckout">
<definition-name>app_sf_responsive_cm:component.system.headerCheckout.pagelet2-Component</definition-name>
<display-name xml:lang="de-DE">Header Bezahlvorgang</display-name>
<display-name xml:lang="en-US">Header Checkout</display-name>
