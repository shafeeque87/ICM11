</configuration-parameter>
<configuration-parameter name="MobileLogo">
<definition-name>app_sf_responsive_cm:component.system.headerCheckout.pagelet2-Component-MobileLogo</definition-name>
<value name="MobileLogo" xml:lang="en-US">PlanetOfPlants:/logo@2x.png</value>
</configuration-parameter>
</configuration-parameters>
<slots>
<slot>
<definition-name>app_sf_responsive_cm:slot.header.promotion.pagelet2-Slot</definition-name>
