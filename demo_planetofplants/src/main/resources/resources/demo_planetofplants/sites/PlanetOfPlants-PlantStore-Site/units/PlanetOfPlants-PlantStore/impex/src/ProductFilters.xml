<searchquery-definition name="only_acer_tablets_filter">
<custom-attributes>
<custom-attribute name="displayName" dt:dt="string" xml:lang="en-US">Acer Tablet Products</custom-attribute>
<custom-attribute name="description" dt:dt="string" xml:lang="en-US">This demo product filter selects tablet products of the manufacturer Acer</custom-attribute>
</custom-attributes>
<global-visible>false</global-visible>
<type>product</type>
<searchquery><contextcategoryid></contextcategoryid><range-conditions/><rankings/><sortings><sorting><attribute-name>name</attribute-name><direction>ASCENDING</direction></sorting></sortings><value-conditions><value-condition><attribute-name>ManufacturerName</attribute-name><attribute-value>Acer</attribute-value></value-condition><value-condition><attribute-name>IcecatCategoryID</attribute-name><attribute-value>897</attribute-value></value-condition></value-conditions></searchquery>
</searchquery-definition>

