<searchquery-definition name="only_acer_filter">
<custom-attributes>
<custom-attribute name="displayName" dt:dt="string" xml:lang="en-US">Acer Products</custom-attribute>
<custom-attribute name="description" dt:dt="string" xml:lang="en-US">This demo product filter selects products of the manufacturer Acer</custom-attribute>
</custom-attributes>
<global-visible>true</global-visible>
<type>product</type>
<searchquery><contextcategoryid></contextcategoryid><range-conditions/><rankings/><sortings><sorting><attribute-name>name</attribute-name><direction>ASCENDING</direction></sorting></sortings><value-conditions><value-condition><attribute-name>ManufacturerName</attribute-name><attribute-value>Acer</attribute-value></value-condition></value-conditions></searchquery>
</searchquery-definition>

<searchquery-definition name="manufacturer_ranking">
<custom-attributes>
<custom-attribute name="displayName" dt:dt="string" xml:lang="en-US">Manufacturer Ranking</custom-attribute>
<custom-attribute name="description" dt:dt="string" xml:lang="en-US">this demo filter sorts notebooks by manufacturer rank, rankings for acer and fujitsu defined</custom-attribute>
</custom-attributes>
<global-visible>true</global-visible>
<type>product</type>
<searchquery><contextcategoryid></contextcategoryid><range-conditions/><rankings><ranking><attribute-name>ManufacturerName</attribute-name><percentage>10</percentage><attribute-value>fujitsu</attribute-value></ranking><ranking><attribute-name>ManufacturerName</attribute-name><percentage>5</percentage><attribute-value>acer</attribute-value></ranking></rankings><sortings/><value-conditions/></searchquery>
</searchquery-definition>

<searchquery-definition name="top_seller_products">
<custom-attributes>
<custom-attribute name="displayName" dt:dt="string" xml:lang="en-US">Top Seller Products</custom-attribute>
<custom-attribute name="description" dt:dt="string" xml:lang="en-US">This demo filter selects products by their selling rate.</custom-attribute>
</custom-attributes>
<global-visible>true</global-visible>
<type>product</type>
<searchquery><contextcategoryid>TopSellers@PlanetOfPlants-PlantStore-Specials</contextcategoryid><range-conditions/><rankings/><sortings><sorting><attribute-name>name</attribute-name><direction>ASCENDING</direction></sorting></sortings><value-conditions/></searchquery>
</searchquery-definition>

<searchquery-definition name="top_rated_products">
<custom-attributes>
<custom-attribute name="displayName" dt:dt="string" xml:lang="en-US">Top Rated Products</custom-attribute>
<custom-attribute name="description" dt:dt="string" xml:lang="en-US">This demo filter selects products by their rating.</custom-attribute>
</custom-attributes>
<global-visible>true</global-visible>
<type>product</type>
<searchquery><contextcategoryid></contextcategoryid> 
	<range-conditions>
        <range-condition>
            <attribute-name>AverageRating</attribute-name>
            <attribute-lower-value>4</attribute-lower-value>
            <attribute-upper-value>*</attribute-upper-value>
        </range-condition>
    </range-conditions><rankings/><sortings><sorting><attribute-name>AverageRating</attribute-name><direction>DESCENDING</direction></sorting></sortings><value-conditions/>
</searchquery>
</searchquery-definition>

<searchquery-definition name="new_arrival_products">
<custom-attributes>
<custom-attribute name="displayName" dt:dt="string" xml:lang="en-US">New Arrival Products</custom-attribute>
<custom-attribute name="description" dt:dt="string" xml:lang="en-US">This demo filter selects products by their arrival date.</custom-attribute>
</custom-attributes>
<global-visible>true</global-visible>
<type>product</type>
<searchquery><contextcategoryid></contextcategoryid> 
	<range-conditions>
        <range-condition>
            <attribute-name>ArrivalDate</attribute-name>
            <attribute-lower-value>2015-01-04T00:00:00Z</attribute-lower-value>
            <attribute-upper-value>*</attribute-upper-value>
        </range-condition>
    </range-conditions><rankings/><sortings><sorting><attribute-name>ArrivalDate</attribute-name><direction>DESCENDING</direction></sorting></sortings><value-conditions/>
</searchquery>
</searchquery-definition>

<searchquery-definition name="new_products">
<custom-attributes>
<custom-attribute name="displayName" dt:dt="string" xml:lang="en-US">New Products</custom-attribute>
<custom-attribute name="description" dt:dt="string" xml:lang="en-US">This demo filter gets products based on arrival date.</custom-attribute>
</custom-attributes>
<global-visible>false</global-visible>
<type>product</type>
<searchquery><contextcategoryid></contextcategoryid> 
	<range-conditions>
        <range-condition>
            <attribute-name>ArrivalDate</attribute-name>
            <attribute-lower-value>2015-01-04T00:00:00Z</attribute-lower-value>
            <attribute-upper-value>*</attribute-upper-value>
        </range-condition>
    </range-conditions><rankings/><sortings><sorting><attribute-name>ArrivalDate</attribute-name><direction>DESCENDING</direction></sorting></sortings><value-conditions/>
</searchquery>
</searchquery-definition>
